#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/israelsantiago/Downloads/Code/cursos/data-structures/typescript/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.24.6_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.24.6__jest@27.5.1_typescript@4.9.5/node_modules/ts-jest/node_modules:/Users/israelsantiago/Downloads/Code/cursos/data-structures/typescript/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.24.6_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.24.6__jest@27.5.1_typescript@4.9.5/node_modules:/Users/israelsantiago/Downloads/Code/cursos/data-structures/typescript/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/israelsantiago/Downloads/Code/cursos/data-structures/typescript/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.24.6_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.24.6__jest@27.5.1_typescript@4.9.5/node_modules/ts-jest/node_modules:/Users/israelsantiago/Downloads/Code/cursos/data-structures/typescript/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.24.6_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.24.6__jest@27.5.1_typescript@4.9.5/node_modules:/Users/israelsantiago/Downloads/Code/cursos/data-structures/typescript/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
